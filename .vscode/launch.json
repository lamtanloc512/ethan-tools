{
  "configurations": [
    {
      "type": "java",
      "name": "EzyTheme",
      "request": "launch",
      "cwd": "${workspaceFolder}/ethantool-theme",
      "mainClass": "org.acme.ethantool.web.test.EzyStarterThemeStartupTest",
      "args": ["-Dmaven.test.skip=false", "-jar"]
    },
    {
      "type": "java",
      "name": "EzyAdmin",
      "request": "launch",
      "cwd": "${workspaceFolder}/ethantool-admin-plugin",
      "mainClass": "org.acme.ethantool.admin.test.EzyStarterAdminPluginStartupTest",
      "args": ["-Dmaven.test.skip=false", "-jar"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LivereloadAdmin",
      "runtimeExecutable": "pnpm",
      "args": ["dev:admin"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LivereloadTheme",
      "runtimeExecutable": "pnpm",
      "args": ["dev:theme"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LivereloadAll",
      "runtimeExecutable": "pnpm",
      "args": ["dev"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "UIDev",
      "runtimeExecutable": "pnpm",
      "args": ["dev:admin:ui"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
  ],
  "compounds": [
    {
      "name": "Dev-Admin",
      "configurations": ["LivereloadAdmin", "EzyAdmin"]
    },
    {
      "name": "Dev-Theme",
      "configurations": ["LivereloadTheme", "EzyTheme"]
    },
    {
      "name": "Dev-All",
      "configurations": ["LivereloadAll", "EzyAdmin", "EzyTheme"]
    },
    {
      "name": "Dev-Admin-Ui",
      "configurations": ["UIDev", "EzyAdmin"]
    }
  ]
}
